    <script type="text/brightscript" uri="pkg:/source/utils/logger.brs" />

    <interface>
        <!-- URIs to fetch content -->
        <field
            id="baseUri"
            type="string"
            value="https://cd-static.bamgrid.com/dp-117731241344/" />
        <field
            id="homePostfixUri"
            type="string"
            value="home.json" />
